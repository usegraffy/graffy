{
  "name": "graffy-project",
  "version": "0.16.0",
  "description": "Graffy Project",
  "type": "module",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aravindet/graffy.git"
  },
  "scripts": {
    "format": "biome check --apply .",
    "lint": "biome ci .",
    "package": "./scripts/package.js",
    "start": "node src/example/server.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules NODE_ENV=testing jest",
    "pg:clean": "docker rm -f graffypg",
    "pg:psql": "docker run --name pgrepl -e POSTGRES_PASSWORD=graffy -d postgres:alpine && until docker exec -it pgrepl psql -U postgres; do sleep 0.5; done ; docker rm -f pgrepl"
  },
  "workspaces": [
    "src/*"
  ],
  "author": "aravindet",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/aravindet/graffy/issues"
  },
  "homepage": "https://graffy.org",
  "devDependencies": {
    "@babel/preset-react": "^7.16.7",
    "@biomejs/biome": "^1.5.3",
    "@faker-js/faker": "^7.6.0",
    "@testing-library/react": "^13.4.0",
    "@types/debug": "^4.1.7",
    "@types/jest": "^29.2.4",
    "@types/pg": "^8.6.5",
    "@types/react": "^18.0.26",
    "@vitejs/plugin-react": "^3.0.0",
    "debug": "^4.3.3",
    "express": "^4.18.2",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "lodash": "^4.17.19",
    "merge-async-iterators": "^0.2.1",
    "nanoid": "^4.0.0",
    "p-map": "^5.3.0",
    "pg": "^8.7.1",
    "prop-types": "^15.8.1",
    "puppeteer": "^19.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^3.0.2",
    "sql-template-tag": "^5.0.3",
    "typescript": "^4.9.4",
    "uuid": "^9.0.0",
    "vite": "^4.5.2",
    "ws": "^8.11.0",
    "yargs": "^17.6.2"
  },
  "peerDependencies": {
    "pg": "^8.0.0",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0"
  }
}