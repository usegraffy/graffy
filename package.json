{
  "name": "graffy-project",
  "version": "0.0.2",
  "description": "Graffy Project",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aravindet/graffy.git"
  },
  "scripts": {
    "lint": "eslint scripts src pages",
    "utest": "jest --testPathIgnorePatterns=example",
    "test": "NODE_ENV=testing jest",
    "package": "./scripts/package",
    "typedef": "tsc --allowJs --declaration --emitDeclarationOnly",
    "format": "prettier --write '{src,scripts,example,pages}/**/*.{js,css,html,json,md}' && eslint --fix scripts src pages",
    "next": "NEXTJS=1 next",
    "build": "NEXTJS=1 next build",
    "start": "node --es-module-specifier-resolution=node src/website/server.js",
    "rogue": "node --es-module-specifier-resolution=node src/rogue-admin/server.js",
    "flatc": "flatc --js --es6-js-export --force-empty-vectors -o src/common/binary/ src/common/binary/*.fbs"
  },
  "workspaces": [
    "src/*",
    "example"
  ],
  "author": "aravindet",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/aravindet/graffy/issues"
  },
  "homepage": "https://graffy.js.org",
  "dependencies": {
    "@babel/runtime": "^7.10.4",
    "@babel/runtime-corejs3": "^7.10.4"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@mapbox/rehype-prism": "^0.5.0",
    "@mdx-js/loader": "^1.6.6",
    "@mdx-js/mdx": "^1.6.6",
    "@msgpack/msgpack": "^2.5.1",
    "@next/mdx": "10.0.3",
    "@testing-library/react-hooks": "^3.4.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "debug": "^4.1.1",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "express": "^4.17.1",
    "fakataaar": "^1.0.0",
    "faker": "^5.1.0",
    "flatbuffers": "^1.12.0",
    "global": "^4.4.0",
    "globby": "^11.0.1",
    "graphiql": "^1.0.5",
    "graphql": "^15.2.0",
    "graphql-tag": "^2.10.3",
    "jest": "^26.1.0",
    "lodash": "^4.17.19",
    "merge-async-iterators": "^0.2.1",
    "nanoid": "^3.1.10",
    "next": "10.0.3",
    "pg": "^8.3.3",
    "prettier": "^2.0.5",
    "prop-types": "^15.7.2",
    "puppeteer": "^5.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "rimraf": "^3.0.2",
    "sql-template-tag": "^4.0.0",
    "text-encoder-lite": "^2.0.0",
    "typescript": "^4.2.3",
    "ws": "^7.3.0",
    "yargs": "^16.1.1"
  },
  "peerDependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  }
}
